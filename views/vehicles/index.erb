<h1 style="text-align:center;">Vehicles Stock</h1>
<% for vehicle in @vehicles %>
  <br>
  <img src="<%= vehicle.image %>" alt="<%= vehicle.model %> Image" style="float:right;width:300px;height:300px;">
  <img src="<%= vehicle.image %>" alt="<%= vehicle.model %> Image" style="float:left;width:300px;height:300px;">
  <h3 style="text-align:center;"> Dealership Bought From: <%= vehicle.get_manufacturer_name() %> </h3>
  <h3 style="text-align:center;"> Make: <%= vehicle.make %> </h3>
  <h3 style="text-align:center;"> Model: <%= vehicle.model %> </h3>
  <h3 style="text-align:center;"> Stock Level to Order At: <%= vehicle.min_stock %> </h3>
  <h3 style="text-align:center;"> Stock held: <%= vehicle.quantity %> </h3>
  <h3 style="text-align:center;"> Purchase Price: <%= vehicle.purchase_price %> </h3>
  <h3 style="text-align:center;"> Selling Price: <%= vehicle.selling_price %> </h3>
  <h4 style="text-align:center;"> <a href="/vehicles/<%= vehicle.id %>">Show Vehicle</a> </h4>
  <h4 style="text-align:center;"> <a href="/vehicles/<%= vehicle.id %>/edit">Edit Vehicle</a> </h4>
  <br>
  <hr>
<% end %>
